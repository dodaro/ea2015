<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
	
	
	<bean id="dispenser" class="com.davide.dispenser.model.Dispenser" scope="prototype">
		<property name="list">
			<list value-type="com.davide.dispenser.model.Products">
				<bean id="milk" class="com.davide.dispenser.model.Products">
					<property name="name" value="Milk"></property>
					<property name="price" value="10"></property>
				</bean>
				
				<bean id="chocolate" class="com.davide.dispenser.model.Products">
					<property name="name" value="Chocolate"></property>
					<property name="price" value="20"></property>
				</bean>
				
				<bean id="coffee" class="com.davide.dispenser.model.Products">
					<property name="name" value="Coffee"></property>
					<property name="price" value="5"></property>
				</bean>
			</list>
		</property>
	
		<property name="stateSelection" value="false"></property>
		<property name="idSelect" value="-1"></property>
		<property name="moneyOnMachine" value="0.0"></property>
		<property name="remainder" value="0.0"></property>
	</bean>
</beans>
